    "def solution(strArr):\n",
    "    answer = []\n",
    "    for word in strArr:\n",
    "        if 'ad' in word:\n",
    "            del word\n",
    "        else:\n",
    "            answer.append(word)\n",
    "    return answer\n",
    "\n",
    "strArr = [\"there\",\"are\",\"no\",\"a\",\"ds\"]\n",
    "print(solution(strArr))"
   ]
